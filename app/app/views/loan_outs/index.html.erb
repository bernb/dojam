<p id="notice"><%= notice %></p>

<h1>Loan Outs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @loan_outs.each do |loan_out| %>
      <tr>
        <td><%= link_to 'Show', loan_out %></td>
        <td><%= link_to 'Edit', edit_loan_out_path(loan_out) %></td>
        <td><%= link_to 'Destroy', loan_out, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Loan Out', new_loan_out_path %>
