<div class="museum_object_page museum_object_storage">
  <h2>location</h2>
  <%= link_to t('edit'), wizard_path(:step_museum) %>
  <dl class="dl-horizontal">
    <div class="row">
      <div class="col-3">
        <dt><%= t('museum') %></dt>
      </div>
      <div class="col">
        <dd><%= @museum_object&.museum&.name %></dd>
      </div>
    </div>
    
    <div class="row">
      <div class="col-3">
        <dt><%= t('inv. number') %></dt>
      </div>  
        <div class="col">
					<dd><%= @museum_object&.full_inv_number %></dd>
      </div>
    </div>
    
    <div class="row">
      <div class="col-3">
        <dt><%= t('other inv. number') %></dt>
      </div>
        <div class="col">
        <dd><%= @museum_object&.inv_numberdoa %></dd>
      </div>
    </div>
    
    <div class="row">
      <div class="col-3">
        <dt><%= t('number of objects') %></dt>
      </div>
        <div class="col">
        <dd><%= @museum_object&.amount %></dd>
      </div>
    </div>
    
    <div class="row">
      <div class="col-3">
        <dt><%= t('location') %></dt>
      </div>
        <div class="col">
        <dd><%= @museum_object&.storage&.name %></dd>
      </div>
    </div>
    
    <div class="row">
      <div class="col-3">
        <dt><%= t('location specified') %></dt>
      </div>
        <div class="col">
        <dd><%= @museum_object&.storage_location&.name %></dd>
      </div>
    </div>
  </dl>
</div>
