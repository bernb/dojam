<%= render "layouts/build_steps_layout", step_name: @step do %>
  <%= simple_form_for @museum_object, url: wizard_path do |f| %>
    <%= f.association :termlist_authenticity, label: "authenticity", include_blank: true %>
    <%= f.input :priority, collection: 1..5, label: "priority", include_blank: true %>
    <%= f.input :priority_determined_by, label: "priority determined by" %>
    <%= f.submit "next step", class: "btn btn-primary" %>
  <% end %>
<% end %>
