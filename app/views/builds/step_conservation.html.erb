<%= render "layouts/build_steps_layout", step_name: @step do %>
  <%= simple_form_for @museum_object, url: wizard_path do |f| %>
    <%= f.input :description_conservation, label: "conservation", as: :text %>
    <%= f.input :needs_conservation, label: "conservation needed" %>
    <%= f.input :needs_cleaning, label: "cleaning needed" %>
    <%= f.button :submit, "next step" %>
  <% end %>
<% end %>

