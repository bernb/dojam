<%= render "layouts/build_steps_layout", step_name: @step do %>
    <%= simple_form_for @museum_object, url: wizard_path do |f| %>
      <% @museum_object.materials.each do |m| %>
				<%= f.input :material_specifieds, collection: m.material_specifieds, label: m.name,  include_blank: false, as: :check_boxes %>
      <% end %>
      <%= f.button :submit, "next step" %>
    <% end %>
<% end %>

